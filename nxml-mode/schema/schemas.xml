  <uri pattern="*.xsl" typeId="XSLT"/>
  <uri pattern="*.html" typeId="XHTML"/>
  <uri pattern="*.rng" typeId="RELAX NG"/>
  <uri pattern="*.rdf" typeId="RDF"/>
  <uri pattern="*.fo"  typeId="FO"/>
  <uri pattern="web.xml" typeId="Web App"/>
  <uri pattern="*.svg" typeId="SVG"/>
  <uri pattern="build.xml" typeId="Ant"/>
  <uri pattern="*.x3d" typeId="X3D"/>


